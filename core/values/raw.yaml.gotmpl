{{ if (.Values | getOrNil "releasesRaw" | getOrNil .Release.Name | getOrNil "manifests") }}
manifests: {{ toYaml (index .Values.releasesRaw .Release.Name "manifests") | nindent 2 }}
{{ else }}
manifests:
- apiVersion: v1
  kind: ConfigMap
  metadata:
    name: {{ .Release.Name }}
  data: {}
{{ end }}
